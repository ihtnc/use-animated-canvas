{
  "name": "@ihtnc/use-animated-canvas",
  "type": "module",
  "version": "1.0.9",
  "description": "React canvas element with a built-in render loop",
  "files": [ "dist" ],
  "main": "./dist/use-animated-canvas.umd.cjs",
  "module": "./dist/use-animated-canvas.js",
